    <BuildMacro Include="EngineExe">
      <Value>$(EngineExe)</Value>
    </BuildMacro>
    <BuildMacro Include="GameName">
      <Value>$(GameName)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>