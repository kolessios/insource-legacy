<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <GameDir>scp</GameDir>
    <DebugParams>-console -window -dev -override_vpk +cl_threaded_init 1 +sv_threaded_init 1</DebugParams>
    <SourceDir>E:\Documentos\Proyectos\Woots\Juegos\Source\insource</SourceDir>
    <GameBaseDir>E:\Documentos\Proyectos\Woots\Juegos\Source\game</GameBaseDir>
    <EngineExeDir>E:\Steam\steamapps\common\Alien Swarm</EngineExeDir>
    <EngineExe>swarm.exe</EngineExe>
    <GameName>SCP</GameName>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>SCP</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="GameDir">
      <Value>$(GameDir)</Value>
    </BuildMacro>
    <BuildMacro Include="DebugParams">
      <Value>$(DebugParams)</Value>
